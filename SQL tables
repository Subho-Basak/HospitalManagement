create table appointment(pt_id varchar2(20) primary key,reg_date varchar2(15),fname varchar2(20),mname varchar2(10),lname varchar2(20),gender varchar2(7),marital_status  varchar2(10),spouce varchar2(50),DOB varchar2(20),age varchar2(10),guardian_name varchar2(50),relation varchar2(15),religion varchar2(15),nationality varchar2(20),c_add1  varchar2(100),c_add2 varchar2(100),c_city varchar2(50),c_PO varchar2(50),c_PS varchar2(50),c_municipality varchar2(50),c_zipcode varchar2(15),c_country varchar2(30),c_state  varchar2(30),c_district varchar2(30),c_email1  varchar2(100),c_email2 varchar2(100),c_tel varchar2(15),c_mob varchar2(15),c_guardian_cont varchar2(15),p_add1 varchar2(100),p_add2  varchar2(100),p_city varchar2(50),p_PO varchar2(50),p_PS varchar2(50),p_municipality varchar2(50),p_zipcode varchar2(15),p_country varchar2(30),p_state varchar2(30),p_district varchar2(30),p_tel varchar2(15),p_mob varchar2(15),p_guardian_cont varchar2(15),date_of_app varchar2(15),specialist varchar2(15),disease_desc  varchar2(500),status varchar2(10));
create table bookAmbulance(name varchar2(70),contact_number number(11) primary key,location varchar2(200),reach_time varchar2(20),Ambulance_type varchar2(50),total_cost varchar2(6),status varchar2(20))
create table medicalStore(vms_no varchar2(20) primary key,nomencature varchar2(100),type varchar2(20),strenght varchar2(100),cost int,quantity int)